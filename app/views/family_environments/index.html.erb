<h1>Family Environments</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Relationship</th>
      <th>Name</th>
      <th>Birthday</th>
      <th>Age</th>
      <th>Family</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @my_family_environments.each do |family_environment| %>
      <tr>
        <td><%= family_environment.relationship %></td>
        <td><%= family_environment.name %></td>
        <td><%= family_environment.birthday %></td>
        <td><%= family_environment.age %></td>
        <td><%= family_environment.family %></td>
        <td><%= link_to 'Show', student_family_environment_path(@student.id,family_environment.id), class: "btn btn-outline-success"%></td>
        <td><%= link_to 'Edit', edit_student_family_environment_path(@student.id,family_environment.id), class: "btn btn-outline-success" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Family Environment', new_student_family_environment_path, class: "btn btn-outline-success" %>
